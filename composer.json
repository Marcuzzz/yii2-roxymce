{
	"name" : "navatech/yii2-roxymce",
	"description" : "Integrate tinymce + roxy manager to yii2",
	"keywords" : [
		"yii2",
		"navatech",
		"tinymce",
		"file manager",
		"roxy",
		"fileman"
	],
	"type" : "yii2-extension",
	"license" : "MIT",
	"homepage" : "https://navatech.vn",
	"support" : {
		"source" : "https://github.com/navatech/yii2-roxymce",
		"original" : "http://www.roxyfileman.com/"
	},
	"authors" : [
		{
			"name" : "Lyubomir Arsov",
			"homepage" : "http://www.roxyfileman.com"
		},
		{
			"name" : "Le Phuong",
			"email" : "phuong17889@gmail.com"
		}
	],
	"require" : {
		"ext-gd" : "*",
		"ext-mbstring" : "*",
		"ext-zip" : "*",
		"navatech/yii2-navatech-base" : "@dev",
		"bower-asset/tinymce-dist" : "^4.3",
		"bower-asset/bootstrap-select" : "^1.11",
		"bower-asset/fontawesome" : "^4.6",
		"bower-asset/patternfly-bootstrap-treeview" : "~2.1",
		"2amigos/yii2-file-upload-widget" : "~1.0"
	},
	"conflict" : {
		"skeeks/yii2-assets-auto-compress" : "1.2.*"
	},
	"autoload" : {
		"psr-4" : {
			"navatech\\roxymce\\" : "src"
		}
	},
	"extra" : {
		"bootstrap" : "navatech\\roxymce\\Bootstrap"
	}
}
